{
    "$schema": "https://raw.githubusercontent.com/lenra-io/ex_component_schema/beta/priv/static/draft-lenra.json",
    "$id": "toggle.schema.json",
    "title": "Toggle",
    "description": "Element of type Toggle",
    "type": "component",
    "properties": {
        "type": {
            "description": "The type of the element",
            "enum": [
                "toggle"
            ]
        },
        "value": {
            "description": "the value of the element",
            "type": "boolean"
        },
        "label": {
            "description": "The label of the element",
            "type": "string"
        },
        "disabled": {
            "description": "Whether the element is disabled or not",
            "type": "boolean",
            "default": false
        },
        "onPressed": {
            "$ref": "defs/listener.schema.json"
        }
    },
    "required": [
        "type",
        "value"
    ],
    "additionalProperties": false
}